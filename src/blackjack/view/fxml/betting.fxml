<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" alignment="center" hgap="10" prefHeight="667.0" prefWidth="375.0"
          vgap="10" xmlns="http://javafx.com/javafx/8.0.112" fx:controller="blackjack.controller.BettingController">
   <columnConstraints>
      <ColumnConstraints/>
   </columnConstraints>
   <rowConstraints>
      <RowConstraints/>
   </rowConstraints>
   <AnchorPane fx:id="anchorPane" prefHeight="667.0" prefWidth="375.0">
      <Button fx:id="add100" layoutX="295.0" layoutY="540.0" mnemonicParsing="false" onMouseClicked="#add100"
              prefHeight="50.0" prefWidth="60.0" text="100"/>
      <Button fx:id="add50" layoutX="225.0" layoutY="540.0" mnemonicParsing="false" onMouseClicked="#add50"
              prefHeight="50.0" prefWidth="60.0" text="50"/>
      <Button fx:id="add25" layoutX="155.0" layoutY="540.0" mnemonicParsing="false" onMouseClicked="#add25"
              prefHeight="50.0" prefWidth="60.0" text="25"/>
      <Button fx:id="add10" layoutX="85.0" layoutY="540.0" mnemonicParsing="false" onMouseClicked="#add10"
              prefHeight="50.0" prefWidth="60.0" text="10"/>
      <Button fx:id="add5" layoutX="15.0" layoutY="540.0" mnemonicParsing="false" onMouseClicked="#add5"
              prefHeight="50.0" prefWidth="60.0" text="5"/>
      <Button layoutX="50.5" layoutY="67.0" mnemonicParsing="false" onMouseClicked="#enterMenu" prefHeight="56.0"
              prefWidth="68.0" text="Wróć"/>
      <Button fx:id="switchToFirstRound" layoutX="300" layoutY="250" mnemonicParsing="false"
              onMouseClicked="#enterToFirstRound" prefHeight="60" prefWidth="60" text="DEAL"/>
      <Label fx:id="printBet" layoutX="240" layoutY="600" prefHeight="40" prefWidth="70"
             text="BET 0$" textAlignment="CENTER"/>
      <Label fx:id="printMyCash" layoutX="80" layoutY="600" prefHeight="40" prefWidth="120"
             text="YOUR CASH 1000$"/>
   </AnchorPane>
</GridPane>
